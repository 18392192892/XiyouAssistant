<view class="cet46_query">
	<view class="cet46_query-img">
		<image src="../../assets/images/cet46Query/clip.png"></image>
	</view>
	<view class="cet46_query-main">
		<view class="cet46_query-main-form">
			<view class="cet46_query-main-form-input">
				<image src="../../assets/images/cet46Query/username.svg"></image>
				<input 
					placeholder="请输入考生姓名" 
					placeholder-style="font-size: 30rpx;"
					type="text" />
			</view>
			<view class="cet46_query-main-form-input">
				<image src="../../assets/images/cet46Query/usernumber.svg"></image>
				<input 
					placeholder="请输入考生准考证号" 
					placeholder-style="font-size: 30rpx;"
					type="number" />
			</view>
			<view class="cet46_query-main-form-button">
				<view>
					查询
				</view>
			</view>
		</view>
	</view>
</view>